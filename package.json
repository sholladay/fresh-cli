{
    "name": "fresh-cli",
    "version": "1.1.1",
    "description": "Remove and reinstall your dependencies.",
    "homepage": "https://github.com/sholladay/fresh-cli",
    "main": "index.js",
    "author": {
        "name": "Seth Holladay",
        "url": "https://seth-holladay.com",
        "email": "me@seth-holladay.com"
    },
    "scripts": {
        "test": "xo && ava"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:sholladay/fresh-cli.git"
    },
    "bugs": {
        "url": "https://github.com/sholladay/fresh-cli/issues",
        "email": "me@seth-holladay.com"
    },
    "engines": {
        "node": ">=8"
    },
    "license": "MPL-2.0",
    "bin": {
        "fresh": "cli.js"
    },
    "files": [
        "index.js",
        "cli.js"
    ],
    "dependencies": {
        "del": "^3.0.0",
        "meow": "^3.7.0",
        "ora": "^1.3.0",
        "pkg-dir": "^2.0.0"
    },
    "devDependencies": {
        "ava": "^0.21.0",
        "eslint-config-tidy": "^0.5.0",
        "mkdirtemp": "^1.1.0",
        "read-pkg": "^2.0.0",
        "write-pkg": "^3.1.0",
        "xo": "^0.18.0"
    },
    "keywords": [
        "cli-app",
        "cli",
        "rm",
        "remove",
        "clean",
        "delete",
        "install",
        "reinstall",
        "latest",
        "node-modules",
        "modules",
        "deps",
        "dependency",
        "dependencies",
        "package-lock",
        "lockfile",
        "safe",
        "reliable",
        "deterministic",
        "reproducible"
    ],
    "xo": {
        "extend": "tidy",
        "overrides": [
            {
                "files": "index.js",
                "rules": {
                    "id-match": [
                        "error",
                        "^[a-zA-Z_$][a-zA-Z\\d_]*$"
                    ]
                }
            }
]
    }
}
